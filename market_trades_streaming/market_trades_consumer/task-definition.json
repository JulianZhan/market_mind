{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "runtimePlatform": {
        "operatingSystemFamily": "LINUX",
        "cpuArchitecture": "ARM64"
    },
    "family": "pyspark-consumer",
    "memory": "2 GB",
    "cpu": ".5 vCPU",
    "executionRoleArn": "arn:aws:iam::145723653607:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "pyspark-consumer",
            "image": "public.ecr.aws/k5r8z9q8/pyspark-consumer:0.1.0",
            "essential": true,
            "portMappings": [
              {
                "name": "pyspark-consumer-5002-tcp",
                "containerPort": 5052,
                "hostPort": 5052,
                "protocol": "tcp", 
                "appProtocol": "http"
              }
            ],
            "environmentFiles": [
              {
                  "value": "arn:aws:s3:::market-mind/env/pyspark-consumer.env",
                  "type": "s3"
              }
          ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/pyspark-consumer",
                    "awslogs-region": "ap-southeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
          }]
        
    
}