          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "\nSELECT \n    date_recorded,\n    MAX(CASE WHEN emotion_name = 'neutral' THEN avg_score ELSE NULL END) AS 'neutral',\n    MAX(CASE WHEN emotion_name = 'joy' THEN avg_score ELSE NULL END) AS 'joy',\n    MAX(CASE WHEN emotion_name = 'fear' THEN avg_score ELSE NULL END) AS 'fear',\n    MAX(CASE WHEN emotion_name = 'sadness' THEN avg_score ELSE NULL END) AS 'sadness',\n    MAX(CASE WHEN emotion_name = 'surprise' THEN avg_score ELSE NULL END) AS 'surprise',\n    MAX(CASE WHEN emotion_name = 'anger' THEN avg_score ELSE NULL END) AS 'anger',\n    MAX(CASE WHEN emotion_name = 'disgust' THEN avg_score ELSE NULL END) AS 'disgust'\nFROM\n    reddit_agg \nwhere date_recorded between $__timeFrom() and $__timeTo() \nGROUP BY\n    date_recorded\nORDER BY\n    date_recorded;\n\n",
