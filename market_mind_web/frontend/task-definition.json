{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "runtimePlatform": {
        "operatingSystemFamily": "LINUX",
        "cpuArchitecture": "ARM64"
    },
    "family": "market-mind-frontend",
    "memory": "2 GB",
    "cpu": ".5 vCPU",
    "executionRoleArn": "arn:aws:iam::145723653607:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "market-mind-frontend",
            "image": "public.ecr.aws/k5r8z9q8/market-mind-frontend:0.1.0",
            "essential": true,
            "portMappings": [
              {
                "name": "market-mind-frontend-tcp",
                "containerPort": 3000,
                "hostPort": 3000,
                "protocol": "tcp", 
                "appProtocol": "http"
              }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/market-mind-frontend",
                    "awslogs-region": "ap-southeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
